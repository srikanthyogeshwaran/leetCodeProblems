3190. Find Minimum Operations to Make All Elements Divisible by Three

class Solution {
    public int minimumOperations(int[] nums) {
        int operations = 0;
        for ( int num : nums){
            int rem = num % 3;
         
        if(rem == 1){
            operations +=1;
        }
        else if (rem==2){
            operations +=1;
        }
        }
     return operations;
    }
}
